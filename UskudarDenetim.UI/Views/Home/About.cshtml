@model UskudarDenetim.UI.Models.ModelCompany

<div class="parallax full-width-bg">
    <div class="container">
        <div class="main-title">
            <h1>Hakkımızda</h1>

        </div>
    </div>
</div>

<div class="full-width-section grey1">
    <div class="dt-sc-margin70"></div>
    <div class="container">
        <div class="hr-title dt-sc-hr-invisible-small">
            <h3>Hakkımızda </h3>
            <div class="title-sep"> </div>
        </div>
        <!-- **column - Starts** -->
        <div class="column dt-sc-three-fifth first">
            <p>@Model.About</p>
            <div class="dt-sc-margin25"></div>
            <blockquote class="type1"> <q>@Model.Quota</q> </blockquote>
            <div class="dt-sc-margin25"></div>
       
        </div> <!-- **column - Ends** -->
        <!-- **column - Starts** -->
        <div class="column dt-sc-two-fifth">
            <!-- **recent-gallery-container - Starts** -->
         
        <div class="recent-gallery-container">
                <!-- **recent-gallery - Starts** -->
                <ul class="recent-gallery">
                    <li> <img src="~/Images/CompanyGallery/gallery1.png" alt="image" /> </li>
                    <li> <img src="http://placehold.it/1170x767&text=Blog" alt="image" /> </li>
                    <li> <img src="http://placehold.it/1170x767&text=Blog" alt="image" /> </li>
                    <li> <img src="http://placehold.it/1170x767&text=Blog" alt="image" /> </li>
                </ul> <!-- **recent-gallery - Ends** -->
                <!-- **bx-pager - Starts** -->
                <div id="bx-pager">
                    <a href="#" data-slide-index='0'><img src="~/Images/CompanyGallery/gallery1.png" alt='' title='' /></a>
                    <a href="#" data-slide-index='1'><img src='http://placehold.it/1170x767&text=Blog' alt='' title='' /></a>
                    <a href="#" data-slide-index='2'><img src='http://placehold.it/1170x767&text=Blog' alt='' title='' /></a>
                    <a href="#" data-slide-index='3'><img src='http://placehold.it/1170x767&text=Blog' alt='' title='' /></a>
                </div> <!-- **bx-pager - Ends** -->
            </div><!-- **recent-gallery-container - Ends** -->

        </div> <!-- **column - Ends** -->

    </div> <!-- **container - Ends** -->
    <div class="dt-sc-margin50"></div>
</div><!-- **Full-width-section - Ends** -->
